{
  "name": "@gpa-lab/content-commons-client",
  "version": "5.4.6",
  "main": "index.js",
  "author": "U.S. Department of State, Bureau of Global Public Affairs Digital Lab <gpa-lab@america.gov>",
  "description": "Frontend interface for the public Content Commons site and it's administrative interface.",
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "cy": "cypress open",
    "deploy": "npm run build && npm run start",
    "dev": "node server.js",
    "lint": "eslint --ext .js --quiet ./",
    "prepare": "husky install config/husky",
    "start": "NODE_ENV=production node -r newrelic server.js",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@next/bundle-analyzer": "^10.1.3",
    "@webdeb/next-styles": "^1.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "aws-amplify": "^3.3.26",
    "axios": "^0.21.1",
    "bodybuilder": "^2.4.0",
    "core-js": "^3.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "html-react-parser": "^1.2.4",
    "immutability-helper": "^3.1.1",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "lodash.flowright": "^3.5.0",
    "lscache": "^1.3.0",
    "mime-types": "^2.1.30",
    "moment": "^2.29.1",
    "newrelic": "^7.2.1",
    "next": "^10.1.3",
    "next-cookies": "^2.0.3",
    "next-redux-wrapper": "^6.0.2",
    "next-with-apollo": "^5.1.1",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-linkify": "^1.0.0-alpha",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^2.0.3",
    "semantic-ui-sass": "^2.4.2",
    "subscriptions-transport-ws": "^0.9.18",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/traverse": "^7.13.13",
    "@babel/types": "^7.13.14",
    "@gpa-lab/eslint-config": "^1.4.1",
    "@testing-library/cypress": "^7.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-graphql-tag": "^3.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cypress": "^6.8.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.23.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.5.4",
    "redux-mock-store": "^1.5.4",
    "waait": "^1.0.5"
  },
  "engines": {
    "node": "10"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/static/__mocks__/fileMock.js",
      "\\.(s?css)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          "@babel/plugin-proposal-optional-chaining",
          [
            "module-resolver",
            {
              "root": "./",
              "alias": {
                "components": "./components",
                "hocs": "./hocs",
                "lib": "./lib",
                "static": "./static",
                "styles": "./styles"
              }
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          "@babel/plugin-proposal-optional-chaining",
          [
            "module-resolver",
            {
              "root": "./",
              "alias": {
                "components": "./components",
                "hocs": "./hocs",
                "lib": "./lib",
                "static": "./static",
                "styles": "./styles"
              }
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-proposal-optional-chaining",
          [
            "module-resolver",
            {
              "root": "./",
              "alias": {
                "components": "./components",
                "hocs": "./hocs",
                "lib": "./lib",
                "static": "./static",
                "styles": "./styles"
              }
            }
          ]
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:IIP-Design/content-commons-client.git"
  }
}
